{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "grpgscript",
      "camelcase": "GRPGScript",
      "title": "GRPGScript",
      "scope": "source.grpgscript",
      "file-types": [
        "grpgscript"
      ],
      "injection-regex": "^grpgscript$",
      "class-name": "TreeSitterGrpgscript"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree Sitter for GRPGScript",
    "authors": [
      {
        "name": "Grian",
        "email": "grianguy32@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/grian32/grpg/grpgscript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}